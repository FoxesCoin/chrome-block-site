{
	"manifest_version": 3,
	"name": "Chrome Block Site",
	"version": "0.0.1",
	"permissions": ["tabs", "storage"],
	"options_page": "src/options/options.html",
	"action": {
		"default_popup": "src/popup/popup.html"
	},
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["index.js"],
			"run_at": "document_start"
		}
	]
}
